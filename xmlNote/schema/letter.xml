<?xml version="1.0"?>
<!-- *声明实例命名空间
	 *设置属性：参数（需要使用的命名空间  命名空间使用的XMLSchema位置） -->

<!-- 引用xml -->
<note
        xmlns="http://www.w3schools.com"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.w3schools.com ../letter/letter.xsd">

    <to>Tove</to>
    <from>Server</from>
    <heading>Reminder</heading>
    <body>Don't forget me this weekend!</body>
</note>

